{
  "name": "semaio/laravel-grids",
  "description": "Grid builder for Laravel 5.7+",
  "keywords": [
    "grid",
    "tables",
    "html",
    "laravel",
    "database",
    "eloquent",
    "bootstrap",
    "laravel-5",
    "laravel5"
  ],
  "homepage": "https://github.com/semaio/laravel-grids",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vitalii [Nayjest] Stepanenko",
      "email": "mail@vitaliy.in",
      "role": "Developer"
    },
    {
      "name": "Rouven Alexander Rieker",
      "email": "therouv@googlemail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "~5.7.0|~5.8.0|^6.0",
    "laravelcollective/html": "~5.7.0|~5.8.0|^6.0",
    "nayjest/builder": "~2"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.11.0@dev",
    "friendsofphp/php-cs-fixer": "^2.15@dev",
    "squizlabs/php_codesniffer": "^3.0@dev"
  },
  "suggest": {
    "maatwebsite/excel": "Required to work with Excel export component"
  },
  "autoload": {
    "psr-4": {
      "Nayjest\\Grids\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nayjest\\Grids\\ServiceProvider"
      ]
    }
  },
  "support": {
    "source": "https://github.com/semaio/laravel-grids",
    "issues": "https://github.com/semaio/laravel-grids/issues"
  },
  "minimum-stability": "dev"
}
